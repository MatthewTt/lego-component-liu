ACf,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AACZ;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACd,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AACZ","file":"/Users/jiajieliu/Codes/lego-component/src/components/PropsTable.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"props-table\">\n    <div\n        v-for=\"(item,k) in finalProps\"\n        :key=\"k\"\n        class=\"form-label\"\n    >\n      <span>{{ item.text }}:</span>\n      <component\n          v-if=\"item\"\n          :is=\"item.component\"\n          :[item.valueProp]=\"item.value\"\n          v-bind=\"item.extraProps\"\n          v-on=\"item.events\"\n          class=\"form-component\"\n      >\n        <template v-if=\"item.subComponent\">\n          <component :is=\"item.subComponent\"\n                     v-for=\"(option, key) in item.options\"\n                     :key=\"key\"\n                     :value=\"option.value\"\n          >{{ option.text }}\n          </component>\n        </template>\n      </component>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\" setup>\nimport { computed, defineEmits, defineProps, PropType, VNode } from \"vue\";\nimport { TextDefaultProps } from \"@/defaultProps\";\nimport { reduce } from \"lodash\";\nimport { mapPropsToForm } from \"../propsMap\";\n\ninterface FormProps {\n  component: string;\n  subComponent?: string;\n  value: string;\n  extraProps?: { [key: string]: any };\n  text?: string;\n  options?: { text: string | VNode; value: any }[];\n  initialTransForm?: (val: any) => any;\n  valueProp: string;\n  eventName: string;\n  events: { [key: string]: (e: any) => void };\n}\n\nconst props = defineProps({\n  props: {\n    type: Object as PropType<TextDefaultProps>,\n    required: true\n  }\n})\nconst emit = defineEmits(['change']);\n\nconst finalProps = computed(() => {\n  return reduce(props.props, (result, value, key) => {\n    const newKey = key as keyof TextDefaultProps\n    const item = mapPropsToForm[newKey]\n    if (item) {\n      const { valueProp = 'value', eventName = 'change', initialTransForm, afterTransForm } = item\n      const newItem: FormProps = {\n        ...item,\n        value: initialTransForm ? initialTransForm(value) : value,\n        eventName,\n        valueProp,\n        events: {\n          [eventName]: (e: any) =>\n            emit('change', { key, value: afterTransForm ? afterTransForm(e) : e })\n        }\n      }\n      result[newKey] = newItem\n    }\n    return result\n  }, {} as { [key: string]: FormProps })\n})\n</script>\n\n<style scoped>\n.props-table {\n  padding: 15px;\n  text-align: left;\n}\n\n.form-label {\n  display: flex;\n  align-items: center;\n  margin-bottom: 10px;\n}\n\n.form-label span {\n  width: 28%;\n}\n.form-component {\n  width: 70%;\n}\n</style>\n"]}]}