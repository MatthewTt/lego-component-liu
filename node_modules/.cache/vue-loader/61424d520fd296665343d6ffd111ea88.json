r.vue\";\n\n\ninterface FormProps {\n  component: string;\n  subComponent?: string;\n  value: string;\n  extraProps?: { [key: string]: any };\n  text?: string;\n  options?: { text: string | VNode; value: any }[];\n  initialTransForm?: (val: any) => any;\n  valueProp: string;\n  eventName: string;\n  events: { [key: string]: (e: any) => void };\n}\n\nconst props = defineProps({\n  props: {\n    type: Object as PropType<AllProps>,\n    required: true\n  }\n})\nconst emit = defineEmits(['change']);\n\nconst finalProps = computed(() => {\n  return reduce(props.props, (result, value, key) => {\n    const newKey = key as keyof AllProps\n    const item = mapPropsToForm[newKey]\n    if (item) {\n      const { valueProp = 'value', eventName = 'change', initialTransForm, afterTransForm } = item\n      const newItem: FormProps = {\n        ...item,\n        value: initialTransForm ? initialTransForm(value) : value,\n        eventName,\n        valueProp,\n        events: {\n          [eventName]: (e: any) =>\n            emit('change', { key, value: afterTransForm ? afterTransForm(e) : e })\n        }\n      }\n      result[newKey] = newItem\n    }\n    return result\n  }, {} as { [key: string]: FormProps })\n})\nconst componentList = {\n  'iconSwitch': IconSwitch,\n  'imageProcesser': ImageProcesser\n}\n</script>\n\n<style scoped>\n.props-table {\n  padding: 15px;\n  text-align: left;\n}\n\n.form-label {\n  display: flex;\n  align-items: center;\n  margin-bottom: 10px;\n}\n\n.form-label > span {\n  width: 28%;\n}\n.form-component {\n  width: 70%;\n}\n.form-label.no-text {\n  display: inline-block;\n  width: 32px;\n  height: 32px;\n  margin-right: 10px;\n}\n#item-fontWeight {\n  margin-left: 28%;\n}\n</style>\n"]}]}